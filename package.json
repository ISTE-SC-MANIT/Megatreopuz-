{
    "name": "megatreopuz-server",
    "version": "1.0.0",
    "private": true,
    "description": "The interface between UI and gRPC architecture",
    "scripts": {
        "start": "node lib/index.js",
        "format": "prettier --write \"src/*.ts\"",
        "lint": "eslint \"src/*.ts\"",
        "commit": "commit",
        "build": "tsc",
        "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/index.ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ISTE-SC-MANIT/megatreopuz-server.git"
    },
    "keywords": [
        "megatreopuz"
    ],
    "author": "Yash Mahalwal <yashmahalwal@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ISTE-SC-MANIT/megatreopuz-server/issues"
    },
    "homepage": "https://github.com/ISTE-SC-MANIT/megatreopuz-server#readme",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@commitlint/prompt-cli": "^8.3.5",
        "@types/express": "^4.17.6",
        "@types/node": "^14.0.13",
        "@typescript-eslint/eslint-plugin": "^3.2.0",
        "@typescript-eslint/parser": "^3.2.0",
        "eslint": "^7.2.0",
        "eslint-plugin-import": "^2.21.2",
        "husky": "^4.2.5",
        "prettier": "^2.0.5",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.9.5"
    }
}
